export int add(int a, int b)
	return a + b

export int inc()
	int val = add(int.load(0u), 1)
	int.store(0u, val)
	return val

// Accessing this struct's properties will emit loads/stores starting at address 64
map Object objects[1] at 8

export float getLength(Vector v)
	return float.sqrt(v.x * v.x + v.y * v.y)

export void normalize(int i)
	float len = getLength(objects[i].position)
	float x = objects[i].position.x / len
	float y = objects[i].position.y / len
	objects[i].position.x = x
	objects[i].position.y = y
	objects[i].colors[i].r = 0

export float sumLengths(Vector vs[4])
	float len = 0f
	int i = 0
	while i < 4
		len = len + getLength(vs[i])
		i = i + 1

struct Vector
	float x // The horizontal coordinate
	float y // The vertical coordinate

struct Color
	int r
	int g
	int b
	int a

struct Object
	Vector position
	Color colors[4]